<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.neera.bamboozled">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.neera.bamboozled.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.neera.bamboozled.activities.Activity_Quiz" />
        <activity android:name="com.example.neera.bamboozled.activities.Activity_Media" />
        <activity android:name="com.example.neera.bamboozled.activities.Activity_LR" />
        <activity android:name="com.example.neera.bamboozled.activities.Activity_Instructions" />
        <activity android:name="com.example.neera.bamboozled.activities.Activity_History"></activity>
    </application>

</manifest>